{
  "name": "iturbo",
  "version": "3.0.0",
  "description": "a mutlipurpose waifu bot",
  "main": "build/client.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "dev": "cross-env NODE_ENV=development nodemon",
    "lint": "eslint --ignore-path .gitignore --ext .ts src/",
    "lint:fix": "yarn lint --fix",
    "release": "standard-version",
    "patch": "standard-version --release-as patch",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TurboCheetah/iTurbo.git"
  },
  "author": "Turbo (turbo.ooo)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TurboCheetah/iTurbo/issues"
  },
  "homepage": "https://github.com/TurboCheetah/iTurbo",
  "devDependencies": {
    "@commitlint/cli": "16.3.0",
    "@commitlint/config-conventional": "16.2.4",
    "@types/node": "16.11.35",
    "@typescript-eslint/eslint-plugin": "5.23.0",
    "@typescript-eslint/parser": "5.23.0",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-n": "15.2.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "6.0.0",
    "husky": "7.0.4",
    "lint-staged": "12.4.1",
    "nodemon": "2.0.16",
    "prettier": "2.6.2",
    "standard-version": "9.5.0",
    "ts-node": "10.7.0"
  },
  "dependencies": {
    "@aero/centra": "^1.0.8",
    "@discordx/importer": "^1.1.2",
    "@discordx/pagination": "^1.0.1",
    "@ksoft/api": "^3.1.9",
    "@types/ms": "0.7.31",
    "anilist-node": "^1.11.0",
    "booru": "^2.5.0",
    "chalk": "^4.1.2",
    "cross-env": "^7.0.3",
    "dayjs": "^1.10.7",
    "discord.js": "13.7.0",
    "discordx": "9.6.0",
    "dotenv": "^16.0.0",
    "graphql": "15.x",
    "graphql-request": "^4.0.0",
    "ms": "^2.1.3",
    "nhentai": "^3.0.0",
    "node-osu": "^2.2.1",
    "reflect-metadata": "^0.1.13",
    "taihou": "^2.0.7",
    "trace.moe.ts": "^1.3.8",
    "tsc-alias": "1.6.7",
    "tsconfig-paths": "4.0.0",
    "tslib": "2.4.0",
    "typescript": "4.6.4"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --ignore-path .gitignore --ext .ts src/"
  },
  "packageManager": "pnpm@7.1.0"
}
