{
  "name": "iturbo",
  "version": "3.0.0",
  "description": "a mutlipurpose waifu bot",
  "main": "build/client.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "dev": "cross-env NODE_ENV=development nodemon",
    "lint": "eslint --ignore-path .gitignore --ext .ts src/",
    "lint:fix": "yarn lint --fix",
    "release": "standard-version",
    "patch": "standard-version --release-as patch",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TurboCheetah/iTurbo.git"
  },
  "author": "Turbo (turbo.ooo)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TurboCheetah/iTurbo/issues"
  },
  "homepage": "https://github.com/TurboCheetah/iTurbo",
  "devDependencies": {
    "@commitlint/cli": "15.0.0",
    "@commitlint/config-conventional": "15.0.0",
    "@types/ms": "0.7.31",
    "@types/node": "16.11.11",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-standard": "16.0.3",
    "eslint-config-standard-with-typescript": "21.0.1",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "5.2.0",
    "husky": "7.0.4",
    "lint-staged": "12.1.2",
    "nodemon": "2.0.15",
    "prettier": "2.5.1",
    "standard-version": "9.3.2",
    "ts-node": "10.4.0",
    "tsc-alias": "1.4.2",
    "tsconfig-paths": "3.12.0",
    "typescript": "4.5.2"
  },
  "dependencies": {
    "@aero/centra": "^1.0.8",
    "@discordx/importer": "^1.1.2",
    "@discordx/utilities": "^3.1.5",
    "@ksoft/api": "^3.1.9",
    "anilist-node": "^1.11.0",
    "booru": "^2.5.0",
    "chalk": "^4.1.2",
    "cross-env": "^7.0.3",
    "dayjs": "^1.10.7",
    "discord.js": "^13.3.1",
    "discordx": "^9.0.1",
    "dotenv": "^10.0.0",
    "graphql": "15.x",
    "graphql-request": "^3.6.1",
    "kurasuta": "^3.0.1",
    "ms": "^2.1.3",
    "nhentai": "^3.0.0",
    "node-osu": "^2.2.1",
    "reflect-metadata": "^0.1.13",
    "taihou": "^2.0.6",
    "trace.moe.ts": "^1.3.8"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --ignore-path .gitignore --ext .ts src/"
  },
  "packageManager": "yarn@3.1.1"
}
