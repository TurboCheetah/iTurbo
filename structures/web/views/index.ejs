<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <link rel="stylesheet" type="text/css"
        href="https://cdn.turbo.ooo/iturbo/playlist/vendor/bootstrap/css/bootstrap.min.css" />
    <link rel="stylesheet" type="text/css"
        href="https://cdn.turbo.ooo/iturbo/playlist/fonts/font-awesome-4.7.0/css/font-awesome.min.css" />
    <link rel="stylesheet" type="text/css" href="https://cdn.turbo.ooo/iturbo/playlist/vendor/animate/animate.css" />
    <link rel="stylesheet" type="text/css"
        href="https://cdn.turbo.ooo/iturbo/playlist/vendor/select2/select2.min.css" />
    <link rel="stylesheet" type="text/css"
        href="https://cdn.turbo.ooo/iturbo/playlist/vendor/perfect-scrollbar/perfect-scrollbar.css" />
    <link rel="stylesheet" type="text/css" href="https://cdn.turbo.ooo/iturbo/playlist/css/util.css" />
    <link rel="stylesheet" type="text/css" href="https://cdn.turbo.ooo/iturbo/playlist/css/main.css" />
</head>

<body>
    <div class="limiter">
        <div class="container-table100">
            <div class="wrap-table100">
                <div class="table100 ver1 m-b-110">
                    <div class="table100-head">
                        <table>
                            <thead>
                                <tr class="row100 head">
                                    <th class="cell100 column1">Song</th>
                                    <th class="cell100 column2">Artist</th>
                                    <th class="cell100 column4">Link</th>
                                </tr>
                            </thead>
                        </table>
                    </div>

                    <div class="table100-body js-pscroll">
                        <table>
                            <tbody>
                                <% for (const song of data.songs) { %>
                                    <tr class="row100 body">
                                        <td class="cell100 column1"><a href="<%= song.uri %>" target="_blank"><%= song.title %></a></td>
                                        <td class="cell100 column2"><%= song.author %></a></td>
                                        <td class="cell100 column4"><a href="<%= song.uri %>" target="_blank"><%= song.uri %></a></td>
                                    </tr>
                                    <% } %>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <script src="https://cdn.turbo.ooo/iturbo/playlist/vendor/jquery/jquery-3.2.1.min.js"></script>
    <script src="https://cdn.turbo.ooo/iturbo/playlist/vendor/bootstrap/js/popper.js"></script>
    <script src="https://cdn.turbo.ooo/iturbo/playlist/vendor/bootstrap/js/bootstrap.min.js"></script>
    <script src="https://cdn.turbo.ooo/iturbo/playlist/vendor/select2/select2.min.js"></script>
    <script src="https://cdn.turbo.ooo/iturbo/playlist/vendor/perfect-scrollbar/perfect-scrollbar.min.js"></script>
    <script>
        $('.js-pscroll').each(function () {
            var ps = new PerfectScrollbar(this);

            $(window).on('resize', function () {
                ps.update();
            });
        });
    </script>
    <script src="https://cdn.turbo.ooo/iturbo/playlist/js/main.js"></script>
</body>

</html>